{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build && nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "jasmine": "jasmine",
    "change:server:mode": "ENV=test",
    "test": "npm run build && npm run change:server:mode && db-migrate --env test up && jasmine && db-migrate db:drop test",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "build": "npx tsc",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern node_moduels/",
    "lint:fix": "eslint 'src/**/*.ts' --fix --ignore-pattern node_modules/"
  },
  "author": "mosobhy",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jasmine": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/db-migrate-pg": "0.0.10",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/nodemon": "^1.19.1",
    "@types/pg": "^7.14.7",
    "@types/prettier": "^2.6.0",
    "eslint": "^8.14.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
